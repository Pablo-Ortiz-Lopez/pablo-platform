{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-restricted-syntax": "off",
        "no-console": "off",
        "no-await-in-loop": "off",
        "no-plusplus": "off"
    }
}